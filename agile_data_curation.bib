@article{harichandran_cufa:_2016,
  title = {{{CuFA}}: {{A}} More Formal Definition for Digital Forensic Artifacts},
  volume = {18, Supplement},
  issn = {1742-2876},
  shorttitle = {{{CuFA}}},
  doi = {10.1016/j.diin.2016.04.005},
  abstract = {The term ``artifact'' currently does not have a formal definition within the domain of cyber/digital forensics, resulting in a lack of standardized reporting, linguistic understanding between professionals, and efficiency. In this paper we propose a new definition based on a survey we conducted, literature usage, prior definitions of the word itself, and similarities with archival science. This definition includes required fields that all artifacts must have and encompasses the notion of curation. Thus, we propose using a new term \textendash{} curated forensic artifact (CuFA) \textendash{} to address items which have been cleared for entry into a CuFA database (one implementation, the Artifact Genome Project, abbreviated as AGP, is under development and briefly outlined). An ontological model encapsulates these required fields while utilizing a lower-level taxonomic schema. We use the Cyber Observable eXpression (CybOX) project due to its rising popularity and rigorous classifications of forensic objects. Additionally, we suggest some improvements on its integration into our model and identify higher-level location categories to illustrate tracing an object from creation through investigative leads. Finally, a step-wise procedure for researching and logging CuFAs is devised to accompany the model.},
  timestamp = {2016-10-28T15:01:29Z},
  urldate = {2016-10-28},
  journal = {Digital Investigation},
  author = {Harichandran, Vikram S. and Walnycky, Daniel and Baggili, Ibrahim and Breitinger, Frank},
  month = aug,
  year = {2016},
  keywords = {Artifact definition,CuFA,Curated forensic artifact,Cyber forensics,CybOX,Digital forensics,Forensic artifact,Ontology,Survey,Taxonomy},
  pages = {S125--S137},
  file = {ScienceDirect Full Text PDF:/Users/kbene/Library/Application Support/Zotero/Profiles/osd26ql5.default/zotero/storage/MKHTZPQU/Harichandran et al. - 2016 - CuFA A more formal definition for digital forensi.pdf:application/pdf;ScienceDirect Snapshot:/Users/kbene/Library/Application Support/Zotero/Profiles/osd26ql5.default/zotero/storage/HWU9SB6B/S1742287616300366.html:text/html}
